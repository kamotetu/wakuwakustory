<div class="profile_side_contents">
  <%=link_to establishment_user_path(@user), class: :side_menu_list do%>
    <div class="side_content_top">
      このユーザーの小説一覧
    </div>
  <%end%>
  <%=link_to user_post_list_user_path(@user), class: :side_menu_list do%>
    <div class="side_content">
      このユーザーの投稿一覧
    </div>
  <%end%>
  <div class="side_content_signup_area">
    <%=link_to root_path, class: :side_menu_list do%>
      <div class="side_content_top">
        トップページ
      </div>
    <%end%>
    <%=link_to mypagemain_user_path(current_user), class: :side_menu_list do%>
      <div class="side_content">
        マイページ
      </div>
    <%end%>
    <%=link_to favorite_list_user_path(current_user), class: :side_menu_list do%>
      <div class="side_content">
        お気に入りタイトル一覧
      </div>
    <%end%>
    <%=link_to destroy_user_session_path, method: :delete, data: {confirm: "ログアウトしますか？"}, class: :side_menu_list do%>
      <div class="side_content">
        ログアウト
      </div>
    <%end%>
  </div>
  
</div>