<div class="wrapper">
  <div class="mypagemain_title_area">
    <h1 class="mypagemain_title">マイページ</h1>
  </div>
  <h1 class="mypagemain_explanation">投稿一覧</h1>
  <div class="mypagemain_container">
    <div class="mypagemain_main_contents">
      <%@stories.each do |story|%>
        <div class="mystory_titles_area">
          <%=link_to story_path(story), class: :mystory_titles do%>
            <div class="mystory_title">
            ○　<%=story.title%>
            </div>
          <%end%>
          <div class="mystory_title_tags">
            <%=render 'shared/mypage_tag_list', tag_list: story.tag_list%>
          </div>
          <div class="mystory_upload_time">
            <%=story.created_at.strftime('%Y/%m/%d %H:%M')%>
          </div>
        </div>
      <%end%>
      <div class="mypage_paginate">
        <%=paginate(@stories)%>
      </div>
    </div>
    <div class="mypagemain_side_contents">
      <div class="side_content_top">
        <%=link_to root_path do%>
          トップページ
        <%end%>
      </div>
      <div class="side_content">
        <%=link_to new_story_path do%>
          投稿する
        <%end%>
      </div>
      <div class="side_content">
        <%=link_to edit_user_registration_path do%>
          プロフィールを編集する
        <%end%>
      </div>
      <div class="side_content">
        <%=link_to destroy_user_session_path, method: :delete, data: {confirm: "ログアウトしますか？"} do%>
          ログアウト
        <%end%>
      </div>
    </div>
  </div>
</div>